{
  "$schema": "https://json.schemastore.org/package",
  "name": "portfolio",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "description": "React-Ts + Tailwind portfolio Template",
  "keywords": [
    "react",
    "vite",
    "typescript",
    "portfolio"
  ],
  "author": {
    "name": "Ziba Nouri",
    "email": "zn.zibanouri@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zibanouri/zibanouri.git"
  },
  "bugs": {
    "url": "https://github.com/zibanouri/zibanouri/issues"
  },
  "homepage": "https://zibanouri.github.io/zibanouri/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:ci": "eslint . --max-warnings=0"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "lucide-react": "^0.526.0",
    "prettier": "^3.6.2",
    "react-router-dom": "^7.7.1",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
